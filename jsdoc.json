{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)",
    "include": ["src", "package.json", "README.md"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "search": true,
    "navLinks": [{
      "label": "Github",
      "href": "https://github.com/arizonatribe/vanillas"
    }],
    "better-docs": {
      "name": "Vanillas",
      "title": "Simple and sweet utility functions for JavaScript and TypeScript applications",
      "navigation": [{
        "label": "Github",
        "href": "https://github.com/arizonatribe/vanillas"
      }]
    }
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "recurse": true,
    "template": "node_modules/better-docs"
  }
}
