{
  "name": "vanillas",
  "description": "Dependency-free, low-level JavaScript function helpers",
  "main": "src/index.js",
  "version": "1.4.0",
  "private": true,
  "license": "MIT",
  "author": "David Nunez <arizonatribe@gmail.com>",
  "types": "types.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/arizonatribe/vanillas.git"
  },
  "keywords": [
    "vanillajs",
    "javascript",
    "performance",
    "helpers",
    "vanillas",
    "function",
    "functional",
    "object",
    "compose",
    "curry",
    "composition",
    "utils",
    "utilities"
  ],
  "watch": {
    "docs": "src/*"
  },
  "scripts": {
    "dist": "npm publish build",
    "docs": "rimraf docs/* && jsdoc --configure jsdoc.json && mv docs/$npm_package_name/$npm_package_version/* docs/ && rimraf docs/$npm_package_name",
    "docs:server": "live-server --open=docs/ --watch=src,docs --port=5000",
    "start": "npm run test:watch",
    "start:docs": "npm run docs && concurrently \"npm-watch\" \"npm run docs:server\"",
    "test:watch": "nodemon test/index.js",
    "test": "tape --require=@babel/register test/index.js",
    "benchmark": "node benchmark",
    "benchmark:docs": "node benchmark --update-docs",
    "benchmark-class": "node --expose-gc benchmark/classMemoryTest.js",
    "lint": "eslint src test benchmark scripts",
    "lint:type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "build": "node scripts/build"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/node": "7.0.0",
    "@babel/plugin-external-helpers": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-stage-0": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-eslint": "8.2.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "benchmark": "2.1.4",
    "better-docs": "^2.0.0",
    "chalk": "2.4.1",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "cross-env": "5.1.4",
    "cross-spawn": "6.0.5",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "^30.7.3",
    "faker": "5.1.0",
    "faucet": "0.0.1",
    "fs-extra": "7.0.0",
    "fs-readdir-recursive": "1.1.0",
    "fuzzy": "0.1.3",
    "fuzzy-search": "3.2.1",
    "fuzzyjs": "4.0.4",
    "glob": "7.1.1",
    "is-promise": "^2.2.2",
    "joi": "^14.0.2",
    "jsdoc": "^3.6.7",
    "jsdoc-export-default-interop": "^0.3.1",
    "live-server": "1.2.1",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.9",
    "npm-watch": "^0.10.0",
    "ramda": "^0.25.0",
    "redux": "4.0.0",
    "rimraf": "^3.0.0",
    "rollup": "~2.38.0",
    "rollup-plugin-terser": "^7.0.0",
    "spected": "^0.7.0",
    "tape": "4.9.1",
    "typescript": "^4.3.5",
    "yup": "^0.32.9"
  }
}
